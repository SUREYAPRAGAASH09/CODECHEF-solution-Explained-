#include <iostream>
using namespace std;

int main() {
	int T, A, B, i;
	cin >> T;
	
	for (i=0; i<T; i++)
	{
	    cin >> A >> B;
	    
	    if ( (A < B) && !(A > B) && !(A == B) )
	        std::cout << "<" << std::endl;
	    else if ( (A > B) && !(A < B) && !(A == B) )
	        std::cout << ">" << std::endl;
	    else
	        std::cout << "=" << std::endl;
	        
	}
	return 0;
}
